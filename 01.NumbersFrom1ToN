using System;

public class NumberFrom1ToN
{
   public static void Main()

    {
	   int startNum = 1;
	   int inputNum = int.Parse(Console.ReadLine());
	   if (inputNum<=0)
	   {
		   Console.WriteLine("number must be a positive");
	   }
	   while ( startNum <= inputNum)
	   {
		   
	   		Console.Write(startNum + " ");
		   startNum ++;
	   }
    }
}
